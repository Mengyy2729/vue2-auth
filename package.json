  },
  "homepage": "https://github.com/searsaw/vue2-auth#readme",
  "dependencies": {
    "body-parser": "^1.15.1",
    "express": "^4.13.4",
    "express-session": "^1.13.0",
    "flash": "^1.1.0",
    "jsonwebtoken": "^7.0.0",
    "node-uuid": "^1.4.7",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "pug": "^2.0.0-beta3",
    "vue": "^2.0.0-alpha.6"
